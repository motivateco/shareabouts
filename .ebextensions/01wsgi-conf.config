option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: wsgi_divvy
container_commands:
  replace_wsgi_config:
    command: "sudo cp /opt/python/current/app/.apache-conf/wsgi_custom.conf /etc/httpd/conf.d/wsgi_custom.conf"
    cwd: /opt/python/current/app/
packages:
  yum:
    postgresql93-server: []
    postgresql93-devel: []
    postgresql93-contrib: []
    git: []
commands:
  01_apache_restart:
    command: "service httpd restart"
